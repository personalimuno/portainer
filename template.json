{
    "version": "2",
    "templates": [
        {
            "type": 1,
            "name": "PostgreSQL",
            "title": "PostgreSQL",
            "description": "The most advanced open-source database",
            "categories": ["database"],
            "platform": "linux",
            "logo": "https://portainer-io-assets.sfo2.digitaloceanspaces.com/logos/postgres.png",
            "image": "postgres:latest",
            "network": "private",
            "env": [
              {
                "name": "POSTGRES_USER",
                "label": "Superuser"
              },
              {
                "name": "POSTGRES_PASSWORD",
                "label": "Superuser password"
              }
            ],
            "ports": ["5432/tcp"],
            "volumes": [
              {
                "/stacks/postgresql/data": "/var/lib/postgresql/data"
              }
            ]
        }
    ]
}